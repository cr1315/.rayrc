{
  /**
   * language specific
   */
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.renderWhitespace": "trailing",
    "files.trimTrailingWhitespace": false
  },
  "[shellscript]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  }
}
